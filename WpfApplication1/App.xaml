<Application x:Class="Webplayer.Services.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="SearchAndPlay.xaml">
    <!--http://msdn.microsoft.com/en-us/library/ms743714%28v=vs.110%29.aspx-->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/CircualButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/PreviousButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/PlayPauseButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/NextButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/StopButton.xaml"></ResourceDictionary>

                <ResourceDictionary Source="Styles/ControllArea.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
